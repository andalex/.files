## FILES & DIR.
alias c='clear'
alias ls='ls -la'
alias mkdir='mkdir -v'

## GIT
alias gs='git status'
alias gb='git branch -a'
alias gl='git log'
alias gr='git rebase -i HEAD~$1'
alias gsync='git fetch upstream;git checkout upstream/master;git rebase upstream/master'

## TMUX
alias mux='tmux attach-session -t andalex-dev || tmux new-session -s andalex-dev'